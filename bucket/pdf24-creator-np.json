{
    "version": "11.6.0",
    "homepage": "https://tools.pdf24.org/en/creator",
    "description": "Free and easy to use PDF software with several functions",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.pdf24.org/en/terms-of-use"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.pdf24.org/pdf24-creator-11.6.0.exe#/setup.exe",
            "hash": "444B4EF1FEA5C2D63B1FB6735CF316B320CE6569F015D02E66A89A0BF657F2D5"
        }
    },
    "pre_install": [
        "if (!(is_admin)) {error \"$app requires admin rights to $cmd\"; break}",
        "Write-Host \"Please wait and don't cancel the script...\" -F 'Yellow'",
        "Start-Process \"$dir\\setup.exe\" -Verb 'RunAs' -ArgumentList @('/VERYSILENT', \"/DIR=$dir\", '/ALLUSERS', '/SUPPRESSMSGBOXES')",
        "Start-Sleep -Seconds 22; Stop-Process -Name 'explorer' -Force -ErrorAction 'SilentlyContinue'",
        "Stop-Service -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'; Stop-Process -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'",
        "Remove-Item \"$dir\\setup.exe\" -ErrorAction 'SilentlyContinue'"
    ],
    "pre_uninstall": [
        "if (!(is_admin)) {error \"$app requires admin rights to $cmd\"; break}",
        "Write-Host \"Please wait and don't cancel the script...\" -F 'Yellow'",
        "Stop-Process -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'; Stop-Service -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'",
        "Start-Process \"$dir\\unins000.exe\" -Wait -Verb 'RunAs' -ArgumentList @('/VERYSILENT', '/FORCECLOSEAPPLICATIONS', '/SUPPRESSMSGBOXES')",
        "Stop-Process -Name 'explorer' -Force -ErrorAction 'SilentlyContinue'; Start-Sleep -Seconds 3"
    ],
    "checkver": "Version\\s([\\d.]+)",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.pdf24.org/pdf24-creator-$versions.msi#/Setup.msi_",
                "hash": {
                    "url": "https://creator.pdf24.org/listVersions.php",
                    "find": "<td>$sha256</td>"
                }
            }
        }
    }
}
